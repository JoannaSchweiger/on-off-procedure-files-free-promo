<!--------------- SLIDE CART SCRIPTS ----------------->  
  {%- include 'slideCart' -%}
  <script>
    window.addEventListener('DOMContentLoaded', function(event) {
      console.log('DOM fully loaded and parsed');
      
      /*---- Add/Remove quantity and remove item from minicart event BEGIN ----*/
      $(document).on('click', '.QuantitySelector__Button, .CartItem__Remove', function() {        	
        var getDataref = $(this).attr('data-href'),   
            getDataId = $(this).attr('data-line-id').trim(),
            getDataKey = '&id='+$(this).attr('data-line-id'),
            getDataName = $(this).attr('data-name'); 
        if(getDataName == 'plus') {
          var getDateQty = parseInt($(this).closest('.QuantitySelector').find('input[name="updates[]"]').val())+1;
        }else if( getDataName == 'minus' ) {
          var getDateQty = parseInt($(this).closest('.QuantitySelector').find('input[name="updates[]"]').val())-1;
        } else {
          var getDateQty = 0;
        }
        if (getDataName == 'plus' || getDataName == 'minus') {
          var getUrl = getDataref+getDateQty+getDataKey;
        }else  {
          var getUrl = getDataref;
        }
        updateSlideCart (getUrl, getDataId, getDateQty);
      });
      /*---- Add/Remove quantity and remove item from minicart event END  ----*/
    });  
    
    /*---- UpdateSlideCart function on Add/Remove quantity from miniCart BEGIN ----*/
    function updateSlideCart (dataref, getDataId, getDateQty) {
      console.log(dataref);
      console.log(getDataId+'======'+getDateQty)
      $.ajax({
        url: dataref,
        dataType: "json",
        cache: false,
        success: function(response) {
          var totalPrice = (response.total_price/100).toFixed(2);
          totalPrice = totalPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");                	
          if(getDateQty === 0) {
            $(document).find('input[data-line-id="'+getDataId+'"]').closest('li').remove();
            if(response.item_count === 0) {
              $('#CartCount').remove();
              $('.mini-cart_content-title, .mini-cart_content-wrapper').addClass('display-none');
              $('.js-empty-cart__message').removeClass('display-none hidden').addClass('display-block');  
              slideCart();
            }
            else {
            slideCart();
            }
          } else {
            $(document).find('input[data-line-id="'+getDataId+'"]').val(getDateQty);
            $('.js-cart__subtotal .mini-cart-buttons .money').text('$'+totalPrice);
            $('#CartCount > span').text(response.item_count);
            console.log('enter to changeqty function');    
            slideCart();
          }
        }
      });
    }
    /*---- UpdateSlideCart function on Add/Remove quantity from miniCart END ----*/
    
    /*---- SlideCart toggle function BEGIN ----*/
    function toggleSlideCart() {
      $('.mini-cart_container').toggleClass('showMiniCart');
      $('.miniCart-overlay').toggle();
    }
    /*---- SlideCart toggle function END ----*/
  </script>
  
<!----------- END SLIDE CART SCRIPTS ------------------>
 

